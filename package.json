{
  "name": "creditinfo-sdk",
  "version": "1.0.1",
  "description": " A wrapper for the credit info soap service",
  "main": "dist/index.js",
  "module": "dist/index.m.js",
  "package": "dist/index.umd.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "nodemon --exec ts-node src/index.ts",
    "build": "rm -rf dist && microbundle --tsconfig tsconfig.json --no-sourcemap",
    "lint": "eslint --fix src test",
    "test": "jest"
  },
  "keywords": [],
  "author": "Sunday Brian (https://github.com/Sundaybrian)",
  "repository": {
    "type": "git",
    "url": "https://github.com/Sundaybrian/creditinfo-wrapper"
  },
  "license": "MIT",
  "files": [
    "dist"
  ],
  "dependencies": {
    "soap": "^0.45.0"
  },
  "devDependencies": {
    "@types/jest": "^28.1.7",
    "@types/node": "^18.7.6",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "eslint": "^8.22.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-import-resolver-typescript": "^3.4.2",
    "eslint-plugin-import": "^2.26.0",
    "jest": "^28.1.3",
    "microbundle": "^0.15.1",
    "nodemon": "^2.0.19",
    "supertest": "^6.2.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "imports": {
    "#server/*": "./src/*.js",
    "#api/*": "./src/api/*.js",
    "#controllers/*": "./src/api/controllers/*.js",
    "#routes/*": "./src/api/routes/*.js",
    "#services/*": "./src/api/services/*.js",
    "#lib/*": "./src/lib/*.js",
    "#jobs/*": "./src/jobs/*.js",
    "#db/*": "./src/db/*.js",
    "#models/*": "./src/db/models/*.js",
    "#middlewares/*": "./src/_middlewares/*.js"
  }
}